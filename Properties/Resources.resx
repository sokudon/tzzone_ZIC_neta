@-moz-document url-prefix("https://claude.ai/chat") {
    .flex-row-reverse {
        /*アイコン */
        background-color: #17f0cf26 !important
    }
    .bg-bg-200 {
        /*サイドメニュー */
        background-color: #2e50d826 !important
    }
    .flex_col {
        /*アイコン右上 */
        background-color: #12e11321 !important
    }

    flex-1 {
        /*アイコン右上 */
        background-color: #c0b71726 !important
    }

    [data-theme=claude],
    [data-theme=claude][data-mode=light] {

        --bg-000: #60dcd5d6;
        --bg-100: #5bf0eede;
        --bg-200: #5b94f0f2;
        --bg-300: #62286042;
        --bg-400: #62286242;
        --bg-500: #62285842;
    }
    .font-user-message {

        background-color: #9393b2db;
    }
    .font-claude-message {
        background-color: #5bf0efdb;
    }
    .code-block__code {
        opacity: 0.8;
    }

    .overflow-hidden {
        /*   .tracking-tight   w-full  .overflow-hidden   .flex-1
        opacity: 0.7;*/
        background-size: auto;
        background: url(data:image/jpeg;base64,</value>
